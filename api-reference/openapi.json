          in: path
          description: distinct_id of the user whose preferences should be fetched
          schema:
            type: string
          required: true
        - name: tenant_id
          in: query
          description: to fetch user preferences for a particular brand
          schema:
            type: string
      responses:
        '200':
          description: '200'
          content:
