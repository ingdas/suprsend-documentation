                    example: "database_sync"
                  is_readonly:
                    type: boolean
                    example: false
                  status:
                    type: string
                    example: "active"
                  track_user_entry:
                    type: boolean
                    example: false
                  track_user_exit:
                    type: boolean
                    example: false
                  requested_for_delete:
                    type: boolean
                    example: false
                  created_at:
                    type: string
                    format: date-time
                    example: "2024-02-21T19:10:01.906000Z"
                  updated_at:
                    type: string
