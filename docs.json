      "nodeTypeOptions": [
        {
          "value": "delay",
          "label": "Delay",
          "description": "Pauses workflow execution for a specified duration."
        },
        {
          "value": "batch",
          "label": "Batch",
          "description": "Batches incoming workflow triggers/events for a duration."
        },
        {
          "value": "digest",
          "label": "Digest",
          "description": "Batches and sends summary notifications at a recurring digest schedule."
        },
        {
          "value": "httpapi_webhook",
          "label": "HTTP API Webhook",
          "description": "Makes an HTTP API request to an endpoint."
        },
        {
          "value": "httpapi_fetch",
          "label": "HTTP API Fetch",
          "description": "Fetches data from an API endpoint using GET method only."
        },
        {
          "value": "branch_waituntil",
          "label": "Branch Wait Until",
          "description": "Waits until a condition is met or the maximum time is reached."
        },
        {
          "value": "branch",
          "label": "Branch",
          "description": "Branches workflow execution based on conditions."
        },
        {
          "value": "transform",
          "label": "Transform",
          "description": "Transforms data or generates/overrides variables using scripting languages."
        },
        {
          "value": "timewindow",
          "label": "Time Window",
          "description": "Restricts workflow execution to specific time windows."
        },
        {
          "value": "subscriberlistoperation_adduser",
          "label": "Add User to List",
          "description": "Adds users to a subscriber list."
        },
        {
          "value": "subscriberlistoperation_removeuser",
          "label": "Remove User from List",
          "description": "Removes users from a subscriber list."
        },
        {
          "value": "objectoperation_addsubscription",
          "label": "Add Object Subscription",
          "description": "Adds users as subscribers to an object."
        },
